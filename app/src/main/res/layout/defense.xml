<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/defenseOne">

    <TextView
        android:id="@+id/teamNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_gravity="center_horizontal"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:textSize="25dp"
        />

    <CheckBox
        android:id="@+id/dockingCheck"
        android:theme="@style/TeoCheck"
        android:layout_marginStart="5dp"
        android:paddingStart="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/KateGray"
        android:text="Did the robot dock?"/>

    <SeekBar
        android:id="@+id/dockingSlider"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:max="3"
        android:progress="1"
        />

    <CheckBox
        android:id="@+id/knockingCheck"
        android:theme="@style/TeoCheck"
        android:layout_marginStart="5dp"
        android:paddingStart="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/KateGray"
        android:text="Did the robot knock?"/>

    <SeekBar
        android:id="@+id/knockingSlider"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:max="3"
        android:progress="1"
        />

    <CheckBox
        android:id="@+id/pathblockingCheck"
        android:theme="@style/TeoCheck"
        android:layout_marginStart="5dp"
        android:paddingStart="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/KateGray"
        android:text="Did the robot pathblock?"/>

    <SeekBar
        android:id="@+id/pathblockingSlider"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:max="3"
        android:progress="1"
        />

</LinearLayout>